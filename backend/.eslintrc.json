{
  "env": {
    "browser": true,
    "es2021": true,
    "node": true
  },
  "extends": [
    "airbnb-base"
  ],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "rules": {
    "no-console": "off",
    "no-restricted-syntax": "off",
    "no-await-in-loop": "off",
    "prettier/prettier": "off",
    "quotes": "off",
    "indent": "off",
    "comma-dangle": "off",
    "arrow-parens": "off",
    "no-plusplus": "off",
    "object-curly-newline": "off",
    "implicit-arrow-linebreak": "off",
    "import/newline-after-import": "off",
    "consistent-return": "off",
    "no-unused-vars": "warn",
    "no-trailing-spaces": "off",
    "eol-last": "off"
  }
}